<?xml version="1.0"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
    <file source-language="en" target-language="en" datatype="plaintext" original="file.ext">
        <body>
            <trans-unit id="-1">
                <source>irc.topic</source>
                <target>Topic</target>
            </trans-unit>
            <trans-unit id="0">
                <source>irc.topic_info</source>
                <target>Topic set on %time% by %setter_nick%</target>
            </trans-unit>
            <trans-unit id="1">
                <source>irc.joined_channel</source>
                <target>%actor% (%actor_hostname%) joined the channel</target>
                <note>Used in IRC logs, example: "@Kashike joined the channel"</note>
            </trans-unit>
            <trans-unit id="2">
                <source>irc.kicked_by</source>
                <target>%recipient_actor% was kicked by %actor% (%kick_message%)</target>
                <note>Used in IRC logs, example: "@lol768 was kicked by @Kashike (hello)"</note>
            </trans-unit>
            <trans-unit id="3">
                <source>irc.set_mode</source>
                <target>%actor% sets mode %mode%</target>
                <note>Used in IRC logs, example: "@Kashike sets mode +b *!*@test.com". Please note that %mode% includes the entire last part "+b *!*@test.com".</note>
            </trans-unit>
            <trans-unit id="4">
                <source>irc.known_as</source>
                <target>%original_actor% is now known as %new_actor%</target>
                <note>Used in IRC logs, example: "@drtshock is now known as @Trent"</note>
            </trans-unit>
            <trans-unit id="5">
                <source>irc.left_channel</source>
                <target>%actor% (%actor_hostname%) left the channel (%part_message%)</target>
                <note>Used in IRC logs, example: "lol768 (lol768!lol7681@bnc.lol768.com) left the channel (Leaving)"</note>
            </trans-unit>
            <trans-unit id="6">
                <source>irc.has_quit</source>
                <target>%actor% (%actor_hostname%) has quit (%quit_message%)</target>
                <note>Used in IRC logs, example: "lol768 (lol768!lol7681@bnc.lol768.com) has quit (I hate you all)"</note>
            </trans-unit>
            <trans-unit id="7">
                <source>irc.topic_is</source>
                <target>Topic is: %topic%</target>
                <note>Used in IRC logs, example: "lol768 (lol768!lol7681@bnc.lol768.com) left the channel"</note>
            </trans-unit>
            <trans-unit id="8">
                <source>irc.has_changed_topic_to</source>
                <target>%actor% has changed the topic to: %topic%</target>
                <note>Used in IRC logs, example: "@lol768 has changed the topic to: testing123</note>
            </trans-unit>
            <trans-unit id="9">
                <source>irc.server</source>
                <target>Server</target>
                <note>Used in IRC logs where the server is responsible (this becomes %actor%).</note>
            </trans-unit>
            <trans-unit id="10">
                <source>nav.home</source>
                <target>Home</target>
                <note>Used in navigation (header/footer). Used for homepage link.</note>
            </trans-unit>
            <trans-unit id="11">
                <source>nav.networks</source>
                <target>Networks</target>
                <note>Used in navigation (header/footer). Used for link to list of networks.</note>
            </trans-unit>
            <trans-unit id="12">
                <source>nav.docs</source>
                <target>Docs</target>
                <note>Used in navigation (header/footer). Used for external link to documentation.</note>
            </trans-unit>
            <trans-unit id="13">
                <source>nav.about</source>
                <target>About</target>
                <note>Used in navigation (header/footer). Used for link to 'about' page which describes the project.</note>
            </trans-unit>
            <trans-unit id="14">
                <source>nav.admin</source>
                <target>Admin</target>
                <note>Used in navigation (header/footer). Used for link to administration overview page.</note>
            </trans-unit>
            <trans-unit id="15">
                <source>nav.issues</source>
                <target>Issues</target>
                <note>Used in navigation (header/footer). Used for external link to issue tracker.</note>
            </trans-unit>
            <trans-unit id="16">
                <source>nav.revisions</source>
                <target>Revisions</target>
                <note>Used in navigation (header/footer). Used for link to list of code changes/revisions.</note>
            </trans-unit>
            <trans-unit id="17">
                <source>nav.login</source>
                <target>Login</target>
                <note>Used in navigation (header/footer). Appears for logged out users, allowing them to login.</note>
            </trans-unit>
            <trans-unit id="18">
                <source>nav.logout</source>
                <target>Logout</target>
                <note>Used in navigation (header/footer). Appears for logged in users, allowing them to logout.</note>
            </trans-unit>
        </body>
    </file>
</xliff>
