{
    "name": "Korobi/Web",
    "description": "The web part of Korobi.",
    "homepage": "https://korobi.io/",
    "license": "proprietary",
    "type": "project",
    "require": {
        "laravel/framework": "~5.0dev",
        "laravel/socialite": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
        "mockery/mockery": "dev-master@dev",
        "barryvdh/laravel-ide-helper": "2.0.*@dev",
        "doctrine/dbal": "~2.3"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Korobi\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "app/Test/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev"
}
